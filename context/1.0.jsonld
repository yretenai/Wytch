{
    "@context": {
        "@vocab": "_:",
        "@version": 1.1,

        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "as": "https://www.w3.org/ns/activitystreams#",
        "sec": "https://w3id.org/security#",
        "wytch": "https://purl.org/wytchspace/ns/ap/1.0#",
        "ldp": "http://www.w3.org/ns/ldp#",
        "vcard":"http://www.w3.org/2006/vcard/ns#",

        "PersonalSpace": "wytch:PersonalSpace",
        "RenderableAsset": "wytch:RenderableAsset",
        "GameSession": "wytch:GameSession",
        "GameConnection": "wytch:GameConnection",

        "authorization": {
            "@id": "wytch:authorization",
            "@type": "xsd:string"
        },
        "authorizationScheme": {
            "@id": "wytch:authorizationScheme",
            "@type": "@id"
        },
        "reason": {
            "@id": "wytch:reason",
            "@type": "@id"
        },

        "license": {
            "@id": "wytch:license",
            "@type": "xsd:string"
        },
        "iconDecoration": {
            "@id": "wytch:iconDecoration",
            "@type": "@id"
        },
        "textTransformation": {
            "@id": "wytch:textTransformation",
            "@type": "@id"
        },

        "position": {
            "@id": "wytch:position",
            "@type": "xsd:double",
            "@container": "@list"
        },
        "rotation": {
            "@id": "wytch:rotation",
            "@type": "xsd:double",
            "@container": "@list"
        },
        "scale": {
            "@id": "wytch:scale",
            "@type": "xsd:double",
            "@container": "@list"
        },
        "asset": {
            "@id": "wytch:asset",   
            "@type": "@id"
        },
        "gltfOverride": {
            "@id": "wytch:gltfOverride",
            "@type": "@json"
        }
    }
}
